{
  "name": "fijian-rag-app",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run build:lambdas && npm run cdk:build",
    "build:shared": "cd lambda/shared && npm install && npm run build",
    "build:translator": "cd lambda/agents/translator && npm install && npm run build",
    "build:lambdas": "npm run build:shared && npm run build:translator && npm run build:collection-manager && npm run build:textract",
    "build:collection-manager": "cd lambda/collection-manager && npm install && npm run build",
    "build:textract": "cd lambda/agents/textract-processor && npm install && npm run build",
    "cdk:build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy"
  },
  "dependencies": {
    "@aws-amplify/backend-auth": "^1.5.1",
    "@aws-cdk/aws-amplify-alpha": "^2.186.0-alpha.0",
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-runtime": "^3.485.0",
    "@aws-sdk/client-cloudwatch": "^3.485.0",
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/client-opensearchserverless": "^3.485.0",
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/client-textract": "^3.782.0",
    "@aws-sdk/credential-provider-node": "^3.787.0",
    "@aws-sdk/lib-dynamodb": "^3.782.0",
    "@heroicons/react": "^2.2.0",
    "@opensearch-project/opensearch": "^2.13.0",
    "@popperjs/core": "^2.11.8",
    "@smithy/protocol-http": "^5.1.0",
    "@smithy/signature-v4": "^2.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node-fetch": "^2.6.12",
    "aws-cdk": "^2.1007.0",
    "aws-cdk-lib": "^2.189.0",
    "bootstrap": "^5.3.3",
    "constructs": "^10.4.2",
    "dotenv": "^16.4.7",
    "langchain": "^0.3.21",
    "next": "latest",
    "node-fetch": "^2.6.9",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.14.3",
    "@aws-amplify/backend-cli": "^1.5.0",
    "@aws-sdk/client-bedrock-runtime": "^3.485.0",
    "@aws-sdk/client-opensearch": "^3.787.0",
    "@aws-sdk/types": "^3.775.0",
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
